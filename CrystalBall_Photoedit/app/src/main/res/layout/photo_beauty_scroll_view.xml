<?xml version="1.0" encoding="utf-8"?>
<HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/hsv_sub_toolbar_beauty"
    android:layout_width="fill_parent"
    android:layout_height="@dimen/bottom_sub_tool_bar_size"
    android:overScrollMode="never"
    android:scrollbars="none">


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <RelativeLayout
            android:id="@+id/button_beauty"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dip"
            android:layout_marginStart="15dip"
            android:layout_weight="1"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onClickSubMenuItem">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/tool_button_tone" />

            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView1"
                android:layout_centerHorizontal="true"
                android:text="얼굴보정"
                android:textColor="@color/color_hsv_menu_text"
                android:textSize="11dip" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/button_acne"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dip"
            android:layout_marginStart="15dip"
            android:layout_weight="1"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onClickSubMenuItem">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/tool_button_acne" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView2"
                android:layout_centerHorizontal="true"
                android:text="기미제거"
                android:textColor="@color/color_hsv_menu_text"
                android:textSize="11dip" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/button_forehead"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dip"
            android:layout_marginStart="15dip"
            android:layout_weight="1"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onClickSubMenuItem">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/tool_button_forehead" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView8"
                android:layout_centerHorizontal="true"
                android:text="이마높이"
                android:textColor="@color/color_hsv_menu_text"
                android:textSize="11dip" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/button_eye_size"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dip"
            android:layout_marginStart="15dip"
            android:layout_weight="1"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onClickSubMenuItem">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/tool_button_eyesize" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView4"
                android:layout_centerHorizontal="true"
                android:text="눈크기"
                android:textColor="@color/color_hsv_menu_text"
                android:textSize="11dip" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/button_norse"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dip"
            android:layout_marginStart="15dip"
            android:layout_weight="1"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onClickSubMenuItem">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/tool_button_norse" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView6"
                android:layout_centerHorizontal="true"
                android:text="코넓이"
                android:textColor="@color/color_hsv_menu_text"
                android:textSize="11dip" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/button_lips"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dip"
            android:layout_marginStart="15dip"
            android:layout_weight="1"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onClickSubMenuItem">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/tool_button_lips" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView7"
                android:layout_centerHorizontal="true"
                android:text="입술크기"
                android:textColor="@color/color_hsv_menu_text"
                android:textSize="11dip" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/button_faceslim"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dip"
            android:layout_marginStart="15dip"
            android:layout_weight="1"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onClickSubMenuItem">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/tool_button_face_slim" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView3"
                android:layout_centerHorizontal="true"
                android:text="턱넓이"
                android:textColor="@color/color_hsv_menu_text"
                android:textSize="11dip" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/button_jaw"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dip"
            android:layout_marginStart="15dip"
            android:layout_weight="1"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onClickSubMenuItem">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/tool_button_jaw" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView5"
                android:layout_centerHorizontal="true"
                android:text="턱길이"
                android:textColor="@color/color_hsv_menu_text"
                android:textSize="11dip" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/button_height"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dip"
            android:layout_marginStart="15dip"
            android:layout_weight="1"
            android:clickable="true"
            android:gravity="center"
            android:onClick="onClickSubMenuItem">

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="40dip"
                android:layout_height="40dip"
                android:layout_centerHorizontal="true"
                app:srcCompat="@drawable/tool_button_height" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView9"
                android:layout_centerHorizontal="true"
                android:text="늘구기"
                android:textColor="@color/color_hsv_menu_text"
                android:textSize="11dip" />
        </RelativeLayout>

    </LinearLayout>
</HorizontalScrollView>